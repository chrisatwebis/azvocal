name = 'azvocal'
description = Uses the Bootstrap Framework Sass source files and must be compiled (not for beginners).
core = 7.x
base theme = bootstrap


;;;;;;;;;;;;;;;;;;;;;
;; Regions
;;;;;;;;;;;;;;;;;;;;;

regions[mobile_menu]    = 'Mobile Menu'
regions[header]         = 'Header'
regions[navigation]     = 'Navigation'
；regions[nav_second]     = 'Secondary Navigation'
regions[featured]     	= 'Featured'
regions[above_content]  = 'Above content'
；regions[highlighted]    = 'Highlighted'
；regions[help]           = 'Help'
regions[content]        = 'Content'
；regions[sidebar_first]  = 'Primary'
；regions[sidebar_second] = 'Secondary'
regions[above_footer]   = 'Above footer'
regions[footer]         = 'Footer'
regions[page_top]       = 'Page top'
regions[page_bottom]    = 'Page bottom'


;;;;;;;;;;;;;;;;;;;;;
;; Stylesheets
;;;;;;;;;;;;;;;;;;;;;

stylesheets[all][] = css/style.css


;;;;;;;;;;;;;;;;;;;;;
;; Scripts
;;;;;;;;;;;;;;;;;;;;;

scripts[] = 'libraries/bootstrap-sass/assets/javascripts/bootstrap/affix.js'
scripts[] = 'libraries/bootstrap-sass/assets/javascripts/bootstrap/alert.js'
scripts[] = 'libraries/bootstrap-sass/assets/javascripts/bootstrap/button.js'
scripts[] = 'libraries/bootstrap-sass/assets/javascripts/bootstrap/carousel.js'
scripts[] = 'libraries/bootstrap-sass/assets/javascripts/bootstrap/collapse.js'
scripts[] = 'libraries/bootstrap-sass/assets/javascripts/bootstrap/dropdown.js'
scripts[] = 'libraries/bootstrap-sass/assets/javascripts/bootstrap/modal.js'
scripts[] = 'libraries/bootstrap-sass/assets/javascripts/bootstrap/tooltip.js'
scripts[] = 'libraries/bootstrap-sass/assets/javascripts/bootstrap/popover.js'
scripts[] = 'libraries/bootstrap-sass/assets/javascripts/bootstrap/scrollspy.js'
scripts[] = 'libraries/bootstrap-sass/assets/javascripts/bootstrap/tab.js'
scripts[] = 'libraries/bootstrap-sass/assets/javascripts/bootstrap/transition.js'
scripts[] = 'js/compiled.min.js'


; Disable the CDN provider so compiled source files can be used.
settings[bootstrap_cdn] = ''
